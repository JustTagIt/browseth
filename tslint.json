{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "object-literal-sort-keys": false,
    "no-namespace": false,
    "no-var-requires": false,
    "max-classes-per-file": false,
    "ban-types": false,
    "variable-name": false
  },
  "rulesDirectory": []
}
